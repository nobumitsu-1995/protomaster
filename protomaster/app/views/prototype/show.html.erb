      <table>
        <tr>
          <th>形状</th>
          <td><%= @name.formtype %></td>
        </tr>
        <tr>
          <th>品名</th>
          <td><%= @name.name %></td>
        </tr>
        <tr>
          <th>顧客名</th>
          <td><%= @client.name %></td>
        </tr>
        <tr>
          <th>配合番号</th>
          <td><%= @proto.number %></td>
        </tr>
        <tr>
          <th>作製日</th>
          <td><%= @proto.measureDate %></td>
        </tr>
        <tr>
          <th>粒度</th>
          <td><%= @proto.particle %></td>
        </tr>
        <tr>
          <th>加工条件</th>
          <td><%= @proto.conditions %></td>
        </tr>
        <tr>
          <th>メモ</th>
          <td><%= @proto.memo %></td>
        </tr>
      </table>
    </div>
